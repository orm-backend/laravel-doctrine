{
    "name": "it-aces/laravel-doctrine",
    "description": "Adds support for Doctrine integration",
    "type": "library",
    "license": "MIT",
    "keywords": ["it-aces", "laravel-doctrine", "laravel", "doctrine"],
    "authors": [
        {
            "name": "Vitaliy Kovalenko",
            "email": "vvk@kola.cloud"
        }
    ],
    "require": {
    	"php": "^7.2.5",
    	"laravel/passport": "^8.0",
    	"laravel-doctrine/orm": "^1.5",
    	"laravel-doctrine/extensions": "^1.1",
    	"beberlei/doctrineextensions": "^1.2",
    	"nesbot/carbon": "^2.0"
    },
    "require-dev": {
    },
    "autoload": {
        "psr-4": {
        	"ItAces\\": "src/ItAces/",
        	"ItAces\\Database\\Seeds\\": "database/seeds/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "extra": {
	    "laravel": {
	        "providers": [
	            "ItAces\\PackageServiceProvider"
	        ]
	    }
	}
}
